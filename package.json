{
  "name": "ideator",
  "version": "1.0.0",
  "description": "Node.js web application to help you find creative ideas using Express, React, D3 and Neo4j.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hayleykwan/ideator.git"
  },
  "engines": {
    "node": "6.10.0",
    "npm": "4.2.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./src/server/index.js",
    "dev": "DEBUG=ideator:* nodemon ./src/server/index.js",
    "build": "webpack",
    "build:hot": "webpack-dev-server --hot",
    "build:dev": "webpack --env=dev --progress --colors",
    "build:prod": "webpack --env=prod --progress --colors"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "d3": "^4.8.0",
    "datamuse": "^1.0.5",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "material-ui": "^0.17.4",
    "morgan": "~1.8.1",
    "natural": "^0.5.1",
    "neo4j-driver": "^1.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "serve-favicon": "~2.4.2",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "author": "Hayley Weng Hei Kwan",
  "keywords": [
    "node",
    "express",
    "react",
    "d3",
    "neo4j"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5"
  }
}
